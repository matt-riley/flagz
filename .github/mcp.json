{
  "mcpServers": {
    "postgres-local": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${FLAGZ_MCP_DATABASE_URL_LOCAL}"
      ]
    },
    "postgres-compose": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${FLAGZ_MCP_DATABASE_URL_COMPOSE}"
      ]
    },
    "github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "github-agentic-workflows": {
      "command": "gh",
      "args": [
        "aw",
        "mcp-server"
      ],
      "env": {
        "GITHUB_ACTOR": "${GITHUB_ACTOR}"
      }
    }
  }
}
