{
  "name": "@matt-riley/flagz",
  "version": "0.2.0",
  "description": "Client library for the flagz feature flag service",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./http": {
      "import": "./dist/http/client.js",
      "types": "./dist/http/client.d.ts"
    },
    "./grpc": {
      "import": "./dist/grpc/client.js",
      "types": "./dist/grpc/client.d.ts"
    }
  },
  "files": [
    "dist",
    "proto"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.11.0",
    "@grpc/proto-loader": "^0.7.0 || ^0.8.0"
  },
  "peerDependenciesMeta": {
    "@grpc/grpc-js": {
      "optional": true
    },
    "@grpc/proto-loader": {
      "optional": true
    }
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.8.0",
    "@types/node": "^24.0.0",
    "fast-check": "^4.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
